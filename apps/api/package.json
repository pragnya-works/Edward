{
  "name": "api",
  "version": "1.0.0",
  "description": "Edward API Server",
  "main": "dist/entry.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"API,WORKER\" -c \"cyan,magenta\" \"pnpm dev:api\" \"pnpm dev:worker\"",
    "dev:api": "tsx watch entry.ts",
    "dev:worker": "tsx watch worker.ts",
    "start": "node dist/entry.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "dependencies": {
    "@langchain/core": "^1.1.15",
    "@langchain/google-genai": "^2.1.10",
    "@langchain/openai": "^1.2.2",
    "@workspace/auth": "workspace:^",
    "@workspace/shared": "workspace:^",
    "bullmq": "^5.66.6",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dockerode": "^4.0.9",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "nanoid": "^5.1.6",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "rate-limit-redis": "^4.3.1",
    "tar-stream": "^3.1.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/dockerode": "^4.0.1",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.7",
    "@types/tar-stream": "^3.1.4",
    "concurrently": "^9.2.1",
    "tsx": "^4.19.2",
    "typescript": "5.7.3"
  }
}